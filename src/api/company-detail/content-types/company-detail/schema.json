{
  "kind": "singleType",
  "collectionName": "company_details",
  "info": {
    "singularName": "company-detail",
    "pluralName": "company-details",
    "displayName": "CompanyDetail"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "org_name": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "full_address": {
      "type": "string",
      "required": true,
      "minLength": 2
    },
    "primary_phone": {
      "type": "string",
      "required": true,
      "unique": false,
      "minLength": 1
    },
    "secondary_phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "po_box": {
      "type": "integer"
    },
    "links": {
      "type": "component",
      "repeatable": false,
      "component": "social-links.social-links"
    },
    "primary_logo": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "secondary_logo": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": false
    }
  }
}
